{
  "name": "wundergraph-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run setup && npm run build && run-p dev wundergraph seed",
    "setup": "npm run migrate init",
    "wundergraph": "wunderctl up --debug",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "seed": "wait-on -d 2000 http://localhost:9991 && ts-node ./src/seed/seed.ts && npm run open",
    "build": "wunderctl generate --debug",
    "build:next": "next build",
    "migrate": "prisma format && prisma migrate dev --name",
    "dev": "next dev",
    "check": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@wundergraph/nextjs": "^0.8.8",
    "@wundergraph/sdk": "^0.135.0",
    "dayjs": "^1.11.7",
    "graphql": "^16.3.0",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.42.1",
    "swr": "2.0.0"
  },
  "devDependencies": {
    "@jest/globals": "29.3.1",
    "@types/node": "17.0.15",
    "@types/node-fetch": "2.6.2",
    "@types/react": "18.0.6",
    "autoprefixer": "10.4.13",
    "eslint-config-next": "13.1.2",
    "jest": "29.3.1",
    "node-fetch": "2.6.7",
    "npm-run-all": "4.1.5",
    "open-cli": "7.0.1",
    "postcss": "8.4.21",
    "prisma": "4.8.1",
    "tailwindcss": "3.2.4",
    "ts-jest": "29.0.3",
    "ts-node": "10.8.0",
    "typescript": "4.8.2",
    "wait-on": "6.0.1"
  }
}
